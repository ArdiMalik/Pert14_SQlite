<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.malik.pert14_SQlite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.malik.pert14_SQlite.UpdateActivity"
            android:parentActivityName="com.malik.pert14_SQlite.MainActivity" />
        <activity
            android:name="com.malik.pert14_SQlite.AddActivity"
            android:label="Add Book"
            android:parentActivityName="com.malik.pert14_SQlite.MainActivity" />
        <activity
            android:name="com.malik.pert14_SQlite.MainActivity"
            android:label="Pertemuan 14 Katalog Buku">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>